{
  "name": "whatarmy/watchtowerhq",
  "license": "GPL-3.0",
  "type": "project",
  "description": "The WatchTowerHQ WordPress plugin allows us to monitor, backup, upgrade, and manage your site!",
  "authors": [
    {
      "name": "WhatArmy",
      "email": "c2p@whatarmydev.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "WhatArmy\\Watchtower\\": "src"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/WatchTowerHQ/mysqldump-php"
    }
  ],
  "require": {
    "claudiosanches/wp-autoloader": "1.0",
    "php": ">= 7.4|8.0",
    "dg/mysql-dump": "v1.5.1",
    "shuber/curl": "dev-master",
    "ext-zip": "*",
    "ext-mysqli": "*",
    "ext-json": "*",
    "woocommerce/action-scheduler": "3.5.2",
    "ext-fileinfo": "*",
    "symfony/finder": "v5.4.11",
    "ext-zlib": "*",
    "ext-iconv": "*",
    "symfony/polyfill-iconv": "v1.26.0",
    "symfony/polyfill-mbstring": "v1.26.0",
    "symfony/polyfill-ctype": "v1.26.0",
    "nelexa/zip": "4.0.2",
    "druidfi/mysqldump-php": "dev-main"
  }
}
